{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "max-classes-per-file": false,
    "member-access": false,
    "no-empty": false,
    "no-submodule-imports": [
      true,
      "vue/types/vue",
      "vue/types/options"
    ],
    "only-arrow-functions": [
      true,
      "allow-declarations",
      "allow-named-functions"
    ]
  }
}
